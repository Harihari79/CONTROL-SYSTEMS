clc;
clear;
s=%s;
G1=syslin('c',1,s+2);
G2=syslin('c',4,s);

T1=G1*G2;
disp (T1, "The series transfer function of T1 = ");

T2=G1+G2;
disp (T2, "The parallel transfer function of T2 = ");

T3=G1/(1+G1*G2);
disp (T3, "The positive feedback transfer function of T3 = ");

T4=G1/(1-G1*G2);
disp (T4, "The negative feedback transfer function of T4 = ");
