clc;
s=%s;
z=0;
num=36; den=36+2*z*s+s^2;
TF=syslin('c',num,den);
subplot(221)
t=0:0.1:10;
y1=csim('step',t,TF);
plot(t,y1)
xlabel('Time {t} sec')
ylabel('System response {c(t)}')
title('Undamped system');

z=0.5;
num=36; den=36+2*z*s+s^2;
TF=syslin('c',num,den);
subplot(222)
t=0:0.1:10;
y2=csim('step',t,TF);
plot(t,y2)
xlabel('Time {t} sec')
ylabel('System response {c(t)}')
title('Underdamped system');

z=1;
num=36; den=36+2*z*s+s^2;
TF=syslin('c',num,den);
subplot(223)
t=0:0.1:10;
y3=csim('step',t,TF);
xlabel('Time {t} sec')
ylabel('System response {c(t)}')
plot(t,y1)
title('Critically damped system');

z=10;
num=36; den=36+2*z*s+s^2;
TF=syslin('c',num,den);
subplot(224)
t=0:0.1:10;
y4=csim('step',t,TF);
plot(t,y4)
xlabel('Time {t} sec')
ylabel('System response {c(t)}')
title('Over damped system');

